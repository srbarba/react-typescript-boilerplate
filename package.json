{
  "name": "react-typescript-webpack-boilerplate",
  "version": "1.0.0",
  "description": "React with Typescript and Webpack boilerplate",
  "main": "./build/intex.js",
  "types": "./build/index.d.ts",
  "author": "Juan Pedro Barba Soler",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack --progress --colors --mode=production",
    "build:ts": "tsc --emitDeclarationOnly",
    "build:ts:test": "tsc --emitDeclarationOnly --p ./test/tsconfig.json",
    "test": "npm run build && jest",
    "test:watch": "npm run build && jest --watch",
    "test:cov": "npm run build && jest --coverage",
    "lint:ts": "tslint 'src/**/*.{ts,tsx,js}'",
    "lint:ts:test": "tslint 'test/**/*.{ts,tsx,js}'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "prettier:base": "prettier",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "prettier:check:test": "npm run prettier:base -- --list-different \"test/**/*.{ts,tsx}\"",
    "prettier:write:test": "npm run prettier:base -- --write \"test/**/*.{ts,tsx}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@svgr/webpack": "^4.3.3",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.16",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss": "^7.0.21",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.7.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
